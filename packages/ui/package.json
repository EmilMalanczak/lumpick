{
  "name": "@lumpik/ui",
  "version": "0.0.0",
  "private": true,
  "description": "UI library for Lumpik. Based on create-react-native-library",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "rm -rf lib node_modules",
    "build": "bob build"
  },
  "devDependencies": {
    "@lumpik/eslint-config": "workspace:^0.1.0",
    "@lumpik/prettier-config": "workspace:^0.1.0",
    "@lumpik/ts-config": "workspace:^0.1.0",
    "@types/react": "^18.3.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-native": "^0.76.2",
    "react-native-builder-bob": "^0.32.1",
    "react-native-web": "^0.19.13",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-native": "^0.76.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@lumpik/eslint-config/base",
      "@lumpik/eslint-config/react",
      "@lumpik/eslint-config/react-native"
    ]
  },
  "prettier": "@lumpik/prettier-config"
}
